<?xml version="1.0" encoding="utf-8" standalone="no"?>
<qnx xmlns="http://www.qnx.com/schemas/application/1.0">
    <id>org.demo.qterm</id>
    <filename>qterm</filename>
    <name>Terminal</name>
    <versionNumber>1.0.19</versionNumber>
    <description>Playbook terminal</description>
    <copyright>2011</copyright>

    <initialWindow>
        <systemChrome>none</systemChrome>
        <transparent>false</transparent>
        <autoOrients>true</autoOrients>
        <aspectRatio>landscape</aspectRatio>
    </initialWindow>

    <author>Home</author>
    <authorId>gYAAgJZ5S0RObHOlpRFR1_d5Luo</authorId>
    <permission system="true">run_native</permission>
    <permission>access_shared</permission>
    <category>core.games</category>
    <icon><image>res/icon.png</image></icon>
    <splashscreen>res/splashscreen.png</splashscreen>
    <env var="LD_LIBRARY_PATH" value="app/native/lib"/>
    <env var="QT_PLUGIN_PATH" value="app/native/plugins"/>
    <env var="QML_IMPORT_PATH" value="app/native/imports"/>
</qnx>
